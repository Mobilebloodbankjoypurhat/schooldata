  document.observe("dom:loaded", function() {
    var allLinks = $jQuery('a');
    var innerUrl = 'www.magentocommerce.com';
    var innerEeUrl = 'www.magentocommerce.com';
    allLinks.each(function(lnk){
      if(lnk.href != "#" &&
        lnk.host != innerUrl &&
        lnk.href != "" &&
        lnk.readAttribute('href')!='' &&
        lnk.readAttribute('href')!=null &&
        lnk.host != innerEeUrl) {
        jQuery(lnk).observe('click', function(){
          recordOutboundLink(lnk, 'Go Outbound Links', lnk.href);
        });
