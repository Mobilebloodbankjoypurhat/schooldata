<script type="text/javascript">
    var currentCatId = null;
    var currentFileId = null;
    var currentFileName = null;
    var isBusy = false;

    /* Tabs switch */
    jQuery(function($){
        $('.download-tabs li').click(function(){
            $('.download-tabs li').removeClass('current');
            $(this).addClass('current');
            $('.download-panes > li').hide();
            $('.download-panes > li').eq($(this).index()).fadeIn('fast');
        });
        $(document).ready(function(){
            var url_hash = $(location).attr('hash').replace(/^#/, "");
            if (url_hash.length) {
                tabScroll(url_hash);
            }
            $('a.anchor').on('click', function(){
                tabScroll($(this).attr('name'));
                return false;
            });
        });
        function tabScroll(hash) {
            var element = $('a[name="'+ hash + '"]'),
                current_li = element.parents('.download-panes > li');
            if (!current_li.is(':visible')) {
                $('.download-panes > li').hide();
                current_li.fadeIn('fast');
                $('.download-tabs li').removeClass('current');
                $('.download-tabs > li').eq(current_li.index()).addClass('current');
            }
            $('html, body').animate({
                scrollTop: element.offset().top-120
            }, 500);
            return false;
        }
    });

    /* Process file download */
    function processFileDownload(fileId, catId, fileName, click) {
        if (isBusy) return false;
        isBusy = true;
        if (fileId == "") {
            isBusy = false;
            alert("You must select format in order to continue!");
            return false;
        }
        if (click) {
            try {
                ga('send', 'event', 'Download', 'Click', fileName);
            }
            catch (e) {}
        }
        jQuery('#overlay').show();
        jQuery.ajax({
            type: "GET",
            url: 'https://www.magentocommerce.com/products/downloads/magento/loggedin/',
            success: function( response ) {
                var result = response.evalJSON();
                if (result.isloggedin) {
                    currentCatId = currentFileId = null;
                    jQuery('#tab_how_to_get_started').click();
                    isBusy = false;
                    jQuery('#overlay').hide();
                    startFileDownload(fileId, catId, result.key, fileName);
                } else {
                    currentCatId = catId;
                    currentFileId = fileId;
                    currentFileName = fileName;
                    lightWindow.activateWindow({
                        href: 'https://www.magentocommerce.com/products/downloads/magento/login/',
                        type: 'page',
                        width: 390,
                        height: 590,
                        lbstyle: 'white',
                        onComplete: function() {
                            isBusy = false;
                            jQuery('#overlay').hide();
                            try {
                                ga('send', 'event', 'RequestAuth', 'Present', fileName);
                            }
                            catch (e) {}
                        }
                    });
                    return false;
                }
            }
        });
    }

    function startFileDownload(fileId, catId, formKey, fileName) {
        try {
            ga('send', 'event', 'Download', 'Delivered_web', fileName);
        }
        catch (e) {}
        if (!formKey) {
            formKey = 'Dv599Mc4kdiBJtov';
        }
        var url = 'https://www.magentocommerce.com/products/downloads/magento/downloadFile/file_id/' +
            fileId + '/file_category/' + catId + '/store_id/1' +
            '/form_key/' + formKey;
        return window.location.href = url;
    }
</script>
